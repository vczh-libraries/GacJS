{
    "name": "@gaclib-website/entry",
    "version": "0.0.1",
    "description": "Website Entry",
    "type": "module",
    "main": "./lib/index.js",
    "scripts": {
        "build-ts": "rimraf ./lib && eslint --fix && tsc",
        "build-assets": "copyfiles -u 1 assets/* lib/dist/",
        "esbuild-dev": "node ./lib/esbuild.js",
        "esbuild-ship": "node ./lib/esbuild.js --ship",
        "build": "npm-run-all2 -s build-ts build-assets esbuild-dev",
        "build-ship": "npm-run-all2 -s build-ts build-assets esbuild-ship"
    },
    "license": "Apache-2.0",
    "devDependencies": {
        "@gaclib-shared/eslint-shared": "^0.0.1",
        "npm-run-all2": "^8.0.4"
    },
    "dependencies": {
        "@gaclib/remote-protocol": "^0.0.1",
        "@gaclib/renderer": "^0.0.1",
        "@gaclib-website/remote-protocol-http": "^0.0.1",
        "esbuild": "0.25.8"
    }
}